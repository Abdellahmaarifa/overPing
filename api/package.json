{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/api/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/api/test/jest-e2e.json",
    "seed:achievement": "ts-node ./api/apps/profile/src/seeder/seed.achievement.ts",
    "generate:auth:prisma": "cd ./apps/auth/prisma && prisma generate --schema schema.prisma",
    "generate:profile:prisma": "cd ./apps/profile/prisma && prisma generate --schema schema.prisma",
    "generate:chat:prisma": "cd ./apps/chat/prisma && prisma generate --schema schema.prisma",
    "generate:game:prisma": "cd ./apps/game/prisma && prisma generate --schema schema.prisma",
    "start:auth:migrate:prod": "prisma db push --schema=apps/auth/prisma/schema.prisma && node dist/apps/auth/main",
    "start:profile:migrate:prod": "prisma db push --schema=apps/profile/prisma/schema.prisma && node dist/apps/profile/main",
    "start:chat:migrate:prod": "prisma db push --schema=apps/chat/prisma/schema.prisma && node dist/apps/chat/main",
    "start:game:migrate:prod": "prisma db push --schema=apps/game/prisma/schema.prisma && node dist/apps/game/main",
    "start:gateway:prod": "node dist/apps/gateway/main",
    "start:media:prod": "node dist/apps/media/main",
    "start:matchmaking:prod": "node dist/apps/matchmaking/main"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@fastify/cors": "^7.0.0",
    "@fastify/static": "^6.11.0",
    "@fastify/websocket": "^5.0.0",
    "@nestjs/apollo": "^12.0.9",
    "@nestjs/common": "^10.2.10",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.10",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.2.10",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.3.0",
    "@nestjs/platform-ws": "^10.2.10",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/websockets": "^10.3.0",
    "@p5-wrapper/react": "^4.2.0",
    "@prisma/client": "5.8.1",
    "@types/graphql-upload": "8.0.4",
    "@types/mime": "^3.0.4",
    "@types/p5": "^1.7.0",
    "@types/passport-google-oauth20": "^2.0.13",
    "@types/speakeasy": "^2.0.10",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.3",
    "apollo-server-express": "^3.13.0",
    "argon2": "^0.31.2",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "config": "^3.3.9",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-upload": "^12.0.0",
    "graphql-ws": "^5.14.2",
    "import": "^0.0.6",
    "mime": "^4.0.1",
    "nest": "^0.1.6",
    "p5": "^1.7.0",
    "passport": "^0.6.0",
    "passport-42": "^1.2.6",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "qrcode": "^1.5.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "speakeasy": "^2.0.0",
    "subscriptions-transport-ws": "^0.11.0",
    "webpack": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.5",
    "@types/express": "^4.17.17",
    "@types/fastify-cors": "^2.1.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.12",
    "@types/socket.io-client": "^3.0.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "aws-sdk": "^2.1494.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "html-loader": "^4.2.0",
    "jest": "^29.5.0",
    "mock-aws-s3": "^4.0.2",
    "nock": "^13.3.8",
    "prettier": "^3.0.0",
    "prisma": "5.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@app/rabbit-mq(|/.*)$": "<rootDir>/libs/rabbit-mq/src/$1"
    }
  }
}
